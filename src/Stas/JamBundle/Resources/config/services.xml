<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="stas.jam.jar.admin.class">Stas\JamBundle\Admin\Jam\JarAdmin</parameter>
    </parameters>

    <services>
        <service id="stas.jam.jar.admin" class="%stas.jam.jar.admin.class%">
            <tag name="sonata.admin" manager_type="orm" group="admin.dashboard.main"
                 label_catalogue="messages" label="Jar" />
            <argument />
            <argument>Stas\JamBundle\Entity\Jam\Jar</argument>
            <argument>SonataAdminBundle:CRUD</argument>
        </service>
    </services>

</container>
